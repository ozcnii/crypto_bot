{
  "openapi": "3.0.0",
  "info": {
    "description": "Документация к backend Aenolabs",
    "version": "0.0.4",
    "title": "Aenolabs Docs"
  },
  "paths": {
    "/users": {
      "get": {
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/user"
                  }
                }
              }
            }
          }
        }
      },
      "post":{
        "requestBody":{
          "required":true,
          "content":{
            "application/json":{
              "schema":{
                "type":"object",
                "properties":{
                  "chat_id":{
                    "type":"integer",
                    "format":"int32"
                  },
                  "username":{
                    "type":"string"
                  },
                  "balance":{
                    "type":"integer",
                    "format":"int32"
                  },     
                  "league":{
                    "type":"string"
                  },
                  "boosters":{
                    "type":"array",
                    "items":{
                      "type":"integer",
                      "format":"int32"
                    },
                    "maxItems":5,
                    "minItems":5
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "default": true
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "requestBody":{
          "required":true,
          "content":{
            "application/json":{
              "schema":{
                "type":"object",
                "properties":{
                  "chat_id":{
                    "type":"integer",
                    "format":"int32"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "default": true
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "user": {
        "properties": {
          "balance": {
            "type": "integer",
            "format": "int32"
          },
          "balance_features": {
            "type": "integer",
            "format": "int32"
          },
          "boosters": {
            "type": "array",
            "items": {
              "type": "integer"
            },
            "minItems": 5,
            "maxItems": 5
          },
          "chat_id": {
            "type": "integer",
            "format": "int32"
          },
          "clan": {
            "type": "integer",
            "format": "int32"
          },
          "historycheck": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "league": {
            "type": "string"
          },
          "pnl": {
            "type": "number",
            "format":"float"
          },
          "referals": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "tasks": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "trades": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "username":{
            "type":"string"
          }
        }
      }
    }
  }
}
